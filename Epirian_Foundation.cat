      <characteristics>
        <characteristic characteristicId="105e-ca6f-41c5-7a69" name="MV" value="7"/>
        <characteristic characteristicId="4ef6-6202-2d81-67e2" name="EVS" value="4"/>
        <characteristic characteristicId="a73d-b1d9-cb27-ac65" name="SKL" value="4"/>
